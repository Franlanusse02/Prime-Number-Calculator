import math

def is_prime(num):
  if num < 2 or num == None: 
    return False
  numsqrt = math.floor(math.sqrt(num)) 

  for x in range(2, numsqrt+1): 
    if num%x == 0: 
      return False
    
  return True 
  
user_language = input("Hi! If you are a English speaker, enter the word English. \n¡Hola! Si hablas en español, ingresa la palabra Español. \n Idioma/Language: "
 
if user_language.title() == "English":
   user_range = input("Enter the highest number we should work with: ")
elif user_language.title() == "Spanish":
   user_range = input("Ingresa el numero mas alto con el que quieres que trabajemos: "
   
for i in range(user_range):
  if is_prime(i):
    print(i)
